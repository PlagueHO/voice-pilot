{
  "name": "voicepilot",
  "displayName": "VoicePilot",
  "description": "A conversational coding assistant for VS Code that integrates with GPT-Realtime for voice interaction.",
  "version": "0.1.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:voicepilot.start"
  ],
  "main": "./src/extension.js",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "contributes": {
    "commands": [
      {
        "command": "voicepilot.start",
        "title": "Start VoicePilot"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "voicepilot.start",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    }
  },
  "dependencies": {
    "@azure/identity": "^2.0.0",
    "openai": "^3.0.0",
    "vscode": "^1.60.0"
  },
  "devDependencies": {
    "typescript": "^4.4.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^6.0.1"
  }
}